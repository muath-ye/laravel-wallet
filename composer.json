{
  "name": "muath-ye/laravel-wallet",
  "description": "A wonderful wallet interface for managing your money.",
  "keywords": [
    "muath-ye",
    "laravel",
    "wallet"
  ],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Muath Alsowadi",
      "email": "muath.ye@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "mockery/mockery": "^1.3.3",
    "orchestra/testbench-dusk": "^4|^5|^6",
    "phpunit/phpunit": "^8.5|^9.0",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Muathye\\Wallet\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Muathye\\Wallet\\ServiceProvider"
      ],
      "aliases": {
        "Wallet": "Muathye\\Wallet\\Support\\Facade"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "version": "1.0.0"
}